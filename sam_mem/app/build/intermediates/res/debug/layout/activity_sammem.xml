<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#b0b0b0b0">"
    

    <ImageButton
        android:id="@+id/button1"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/n_bl" />
    
    <ImageButton
        android:id="@+id/button2"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignLeft="@+id/button1"
        android:layout_above="@+id/button1"
        android:src="@drawable/n_l" />
    
    <ImageButton
        android:id="@+id/button3"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignLeft="@+id/button2"
        android:layout_above="@+id/button2"
        android:src="@drawable/n_tl" />
       
    
    <ImageButton
        android:id="@+id/button4"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/button3"
        android:layout_toRightOf="@+id/button3"
        android:src="@drawable/n_t" />
    
    <ImageButton
        android:id="@+id/button5"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/button4"
        android:layout_toRightOf="@+id/button4"
        android:src="@drawable/n_tr" />
    
    <ImageButton
        android:id="@+id/button6"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignRight="@+id/button5"
        android:layout_below="@+id/button5"
        android:src="@drawable/n_r" />
    
    <ImageButton
        android:id="@+id/button7"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignRight="@+id/button6"
        android:layout_below="@+id/button6"
        android:src="@drawable/n_br" />

    <ImageButton
        android:id="@+id/button8"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/button7"
        android:layout_toLeftOf="@+id/button7"
        android:src="@drawable/n_b" /> 
    
    <ImageButton
        android:id="@+id/button9"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginLeft="15dp"
        android:layout_alignBottom="@+id/button7"
        android:layout_toRightOf="@+id/button7"
        android:src="@drawable/n_l_rota" />
    
    <ImageButton
        android:id="@+id/button10"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignBottom="@+id/button9"
        android:layout_toRightOf="@+id/button9"
        android:src="@drawable/n_r_rota" />
    

    <ImageButton
        android:id="@+id/button11"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_alignBottom="@+id/button5"
        android:layout_toRightOf="@+id/button5"
        android:src="@drawable/p_l_rota" />
    
    
    <ImageButton
        android:id="@+id/button14"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/button11"
        android:layout_toRightOf="@+id/button11"
        android:src="@drawable/camera_stop" />
    
    <ImageButton
        android:id="@+id/button12"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/button14"
        android:layout_toRightOf="@+id/button14"
        android:src="@drawable/p_r_rota" />
    
     
    
    <ImageButton
        android:id="@+id/button13"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_above="@+id/button8"
        android:layout_toRightOf="@+id/button1"
        android:src="@drawable/car_stop" />
    
    <Button
        android:id="@+id/connect_button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.18"
        android:text="@string/disconnected" />
    
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_gravity="center"
        android:layout_marginTop="335dp"
        android:layout_marginLeft="110dp" ></ImageView>
       
    
    <TextView
        android:id="@+id/TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="300dp" ></TextView>
    
    <TextView
        android:id="@+id/TextView2"        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="20dp"
        android:text="현재 진행 방향 :" ></TextView> 
        
    <TextView
        android:id="@+id/TextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="350dp"
        android:layout_marginLeft="200dp"
        android:text="웹캠 회전 방향 :" ></TextView>
    
</RelativeLayout>
